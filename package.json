{
  "name": "pimdb",
  "type": "module",
  "module": "src/index.ts",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "prettier --check . && eslint . && vitest run && tsc --noEmit false",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "test": "vitest",
    "benchmark:compare": "vitest bench --compare benchmarks.json",
    "benchmark:store": "vitest bench --run --outputJson benchmarks.json",
    "generate-1k": "bun src/indexes/benchmarks/generate-test-data.ts 1000",
    "generate-10k": "bun src/indexes/benchmarks/generate-test-data.ts 10000",
    "generate-100k": "bun src/indexes/benchmarks/generate-test-data.ts 100000"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/bun": "latest",
    "eslint": "^9.14.0",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0",
    "vitest": "^2.1.5"
  }
}
